{
  "plugins": [],
  "recurseDepth": 10,
  "source": {
    "include": ["src/app"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionares": ["jsdoc", "closure"]
  },
  "templates": {
    "referenceTitle": "Personal Pass",
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "destination": "./docs/",
    "encoding": "utf8",
    "private": true,
    "recurse": true
  }
}
