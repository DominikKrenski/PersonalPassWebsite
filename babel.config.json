{
  "presets": [
    [
      "@babel/preset-env",
      {
        "debug": false,
        "useBuiltIns": "usage",
        "corejs": { "version": "3.18", "proposals": true }
      }
    ],
    [
      "@babel/preset-react",
      {
        "runtime": "automatic",
        "throwIfNamespace": true,
        "importSource": "react"
      }
    ]
  ],
  "plugins": [
    [
      "babel-plugin-react-scoped-css",
      {
        "include": ".local.(sa|sc|c)ss$"
      }
    ]
  ]
}
